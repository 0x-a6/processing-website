  "brief": "Datatype for storing and manipulating video frames from an attached capture device such as a camera.",
  "methods": [
    {
      "anchor": "Capture_frameRate_",
      "name": "frameRate()",
      "desc": "Sets how often frames are read from the capture device. Setting the <b>fps</b>\n parameter to 4, for example, will cause 4 frames to be read per second."
    },
    {
      "anchor": "Capture_available_",
      "name": "available()",
      "desc": "Returns \"true\" when a new frame from the device is available to read."
    },
    {
      "anchor": "Capture_start_",
      "name": "start()",
      "desc": "Starts capturing frames from the selected device."
    },
    {
      "anchor": "Capture_stop_",
      "name": "stop()",
      "desc": "Stops capturing frames from an attached device."
    },
    {
      "anchor": "Capture_read_",
      "name": "read()",
      "desc": "Reads the current frame of the device."
    }
  ],
